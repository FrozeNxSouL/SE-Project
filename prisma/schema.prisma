generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model Productt {
  id    String @id @default(auto()) @map("_id") @db.ObjectId
  email String @map("_email")
  name  String @map("_name")
}


model User {
  id     String  @id @default(auto()) @map("_id") @db.ObjectId
  email  String? @unique
  name   String?
  second String?
}
